from matplotlib import pyplot as plt
from numpy import *

class ecuacionOnda():
    def __init__(self):
        phi = []
        for i in arange(0, 1 / 3, 0.01):
            phi.append(i)
        for i in arange(1 / 3, (2 * 1) / 3, 0.01):
            phi.append(1 / 3)
        for i in arange((2 * 1) / 3, 1, 0.01):
            phi.append(1 - i)
        self.phi = phi

if __name__ == '__main__':
    y = ecuacionOnda()
    